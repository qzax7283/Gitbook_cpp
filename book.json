{
	"styles": {
		"website": "add.css"
	},
	"plugins": [
		"theme-comscore",
		"-sharing",
		"highlight",
		"code",
		"bottom-navigation",
		"page-footer-ex",
		"codeblock-filename",
		"flexible-alerts"
	],
	"pluginsConfig": {
		"fontSettings": {
			"theme": "dark",
			"family": "sans"
		},
		"theme-default": {
			"showLevel": false
		},
		"page-footer-ex": {
			"copyright": "By qzax",
			"markdown": false,
			"update_label": "Last update: ",
			"update_format": "YYYY-MM-DD HH:mm:ss"
		},
		"flexible-alerts": {
			"style": "flat",
			"tip": {
				"label": "Note"
			},
			"warning": {
				"label": "Warning"
			},
			"danger": {
				"label": "Attention"
			}
		}
	}
}